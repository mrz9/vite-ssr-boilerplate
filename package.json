{
    "name": "vite-ssr-boilerplate",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "cross-env APP_MODE=dev vite --config vite.config.server.js",
        "dev:pre": "cross-env APP_MODE=pre vite --config vite.config.server.js",
        "dev:pub": "cross-env APP_MODE=pub vite --config vite.config.server.js",
        "build:dev": "cross-env APP_MODE=dev node build/index",
